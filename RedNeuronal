      "cell_type": "markdown",
      "source": [
        "**Codigo Modificado**"
      ],
      "metadata": {
        "id": "UGYxFKACNfel"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "\n",
        "# === FUNCIONES DE ACTIVACIÓN ===\n",
        "def sigmoid(x):\n",
        "    return 1.0 / (1.0 + np.exp(-x))\n",
        "\n",
        "def sigmoid_derivada(x):\n",
        "    return sigmoid(x) * (1.0 - sigmoid(x))\n",
        "\n",
        "def tanh(x):\n",
        "    return np.tanh(x)\n",
        "\n",
        "def tanh_derivada(x):\n",
        "    return 1.0 - x**2\n",
        "\n",
        "\n",
        "# === CLASE RED NEURONAL ===\n",
        "class NeuralNetwork:\n",
        "\n",
        "    def __init__(self, layers, activation='tanh'):\n",
        "        if activation == 'sigmoid':\n",
        "            self.activation = sigmoid\n",
        "            self.activation_prime = sigmoid_derivada\n",
        "        elif activation == 'tanh':\n",
        "            self.activation = tanh\n",
        "            self.activation_prime = tanh_derivada\n",
        "\n",
        "        self.weights = []\n",
        "        self.deltas = []\n",
        "\n",
        "        # Inicializamos pesos (capas ocultas)\n",
        "        for i in range(1, len(layers) - 1):\n",
        "            r = 2*np.random.random((layers[i-1] + 1, layers[i] + 1)) - 1\n",
        "            self.weights.append(r)\n",
        "\n",
        "        # Inicializamos pesos (capa de salida)\n",
        "        r = 2*np.random.random((layers[i] + 1, layers[i+1])) - 1\n",
        "        self.weights.append(r)\n",
        "\n",
        "    def fit(self, X, y, learning_rate=0.03, epochs=40001):\n",
        "        ones = np.atleast_2d(np.ones(X.shape[0]))\n",
        "        X = np.concatenate((ones.T, X), axis=1)\n",
        "\n",
        "        for k in range(epochs):\n",
        "            i = np.random.randint(X.shape[0])\n",
        "            a = [X[i]]\n",
        "\n",
        "            for l in range(len(self.weights)):\n",
        "                dot_value = np.dot(a[l], self.weights[l])\n",
        "                activation = self.activation(dot_value)\n",
        "                a.append(activation)\n",
        "\n",
        "            error = y[i] - a[-1]\n",
        "            deltas = [error * self.activation_prime(a[-1])]\n",
        "\n",
        "            for l in range(len(a) - 2, 0, -1):\n",
        "                deltas.append(deltas[-1].dot(self.weights[l].T) * self.activation_prime(a[l]))\n",
        "\n",
        "            deltas.reverse()\n",
        "\n",
        "            for j in range(len(self.weights)):\n",
        "                layer = np.atleast_2d(a[j])\n",
        "                delta = np.atleast_2d(deltas[j])\n",
        "                self.weights[j] += learning_rate * layer.T.dot(delta)\n",
        "\n",
        "            if k % 10000 == 0:\n",
        "                print(\"epoch:\", k)\n",
        "\n",
        "    def predict(self, x):\n",
        "        a = np.concatenate((np.array([1.0]), np.array(x)))\n",
        "        for l in range(len(self.weights)):\n",
        "            a = self.activation(np.dot(a, self.weights[l]))\n",
        "        return a\n",
        "\n",
        "# === NUEVA ARQUITECTURA (4 entradas → 5 ocultas → 5 salidas) ===\n",
        "nn = NeuralNetwork([4, 5, 5], activation='tanh')\n",
        "\n",
        "# === NUEVO DATASET DEL AUTO (COMPORTAMIENTO) ===\n",
        "X = np.array([\n",
        "    [-1,-1,-1,-1],  # Libre -> avanzar\n",
        "    [0,1,0,-1],     # obstaculo derecha -> girar izquierda\n",
        "    [1,0,-1,0],     # obstaculo izquierda -> girar derecha\n",
        "    [0,0,1,-1],     # obstaculo frente -> retroceder\n",
        "    [0,0,-1,1],     # obstaculo atrás -> avanzar\n",
        "    [1,1,1,-1],     # muy cerrado adelante -> frenar\n",
        "    [0,0,0,1],      # atrás peligroso -> avanzar\n",
        "])\n",
        "\n",
        "y = np.array([\n",
        "    [1,1,0,0,0],  # avanzar\n",
        "    [1,0,0,1,0],  # giro izquierda\n",
        "    [0,1,1,0,0],  # giro derecha\n",
        "    [0,0,1,1,0],  # retroceder\n",
        "    [1,1,0,0,0],  # avanzar\n",
        "    [0,0,0,0,1],  # frenar\n",
        "    [1,1,0,0,0],  # avanzar\n",
        "])\n",
        "\n",
        "# === ENTRENAMIENTO ===\n",
        "nn.fit(X, y, learning_rate=0.03, epochs=40001)\n",
        "\n",
        "# === FUNCIÓN PARA REDONDEAR LA SALIDA ===\n",
        "def valNN(x):\n",
        "    return int(abs(round(x)))\n",
        "\n",
        "# === PRUEBA DE PREDICCIÓN ===\n",
        "print(\"\\n--- PRUEBAS COMPLETAS ---\")\n",
        "index=0\n",
        "for e in X:\n",
        "    pred = nn.predict(e)\n",
        "    print(\"Entrada:\", e,\n",
        "          \" -> Esperado:\", y[index],\n",
        "          \" Obtenido:\", [valNN(p) for p in pred])\n",
        "    index += 1\n"
      ],
